{
  "name": "buf-svelte",
  "version": "0.0.1",
  "scripts": {
    "start": "npm run buf:generate && vite dev",
    "build": "vite build",
    "test": "vitest run",
    "test:e2e": "npm run playwright:install && playwright test",
    "playwright:install": "playwright install",
    "buf:generate": "npx buf generate buf.build/connectrpc/eliza",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write ."
  },
  "type": "module",
  "devDependencies": {
    "@bufbuild/buf": "^1.26.1",
    "@bufbuild/protobuf": "^1.3.0",
    "@bufbuild/protoc-gen-es": "^1.3.0",
    "@connectrpc/connect": "^0.13.0",
    "@connectrpc/connect-web": "^0.13.0",
    "@connectrpc/protoc-gen-connect-es": "^0.13.0",
    "@playwright/test": "^1.36.2",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.22.5",
    "@testing-library/jest-dom": "^6.1.2",
    "@testing-library/svelte": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-svelte": "^2.33.0",
    "flush-promises": "^1.0.2",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^3.0.3",
    "svelte": "^4.2.0",
    "svelte-check": "^3.4.6",
    "svelte-preprocess": "^5.0.4",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.8",
    "vitest": "^0.34.3"
  }
}
