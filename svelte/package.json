{
    "name": "buf-svelte",
    "version": "0.0.1",
    "scripts": {
        "start": "npm run buf:generate && vite dev --port 3000",
        "build": "vite build",
        "test": "npm run playwright:install && playwright test",
        "playwright:install": "playwright install",
        "buf:generate": "buf generate buf.build/bufbuild/eliza",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --check --ignore-path ../.prettierignore . && eslint .",
        "format": "prettier --write --ignore-path ../.prettierignore ."
    },
    "devDependencies": {
        "@bufbuild/connect-web": "^0.6.0",
        "@bufbuild/protobuf": "^1.0.0",
        "@bufbuild/protoc-gen-connect-web": "^0.6.0",
        "@bufbuild/protoc-gen-es": "^1.0.0",
        "@playwright/test": "^1.30.0",
        "@sveltejs/adapter-auto": "next",
        "@sveltejs/kit": "next",
        "@typescript-eslint/eslint-plugin": "^5.47.1",
        "@typescript-eslint/parser": "^5.27.0",
        "eslint": "^8.27.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-svelte3": "^4.0.0",
        "prettier": "^2.8.3",
        "prettier-plugin-svelte": "^2.8.0",
        "svelte": "^3.53.0",
        "svelte-check": "^3.0.3",
        "svelte-preprocess": "^5.0.1",
        "tslib": "^2.4.1",
        "typescript": "^4.8.4",
        "vite": "^3.1.0"
    },
    "type": "module"
}
