{
  "name": "buf-svelte",
  "version": "0.0.1",
  "scripts": {
    "start": "npm run generate && vite dev",
    "build": "vite build",
    "test": "vitest run",
    "test:e2e": "npm run playwright:install && playwright test",
    "playwright:install": "playwright install",
    "generate": "buf generate buf.build/connectrpc/eliza",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "ci": "npm run generate && npm run check && npm run format && npm run lint && npm run build && npm run test && npm run test:e2e"
  },
  "type": "module",
  "devDependencies": {
    "@bufbuild/buf": "^1.29.0",
    "@bufbuild/protobuf": "^1.7.2",
    "@bufbuild/protoc-gen-es": "^1.7.2",
    "@connectrpc/connect": "^1.3.0",
    "@connectrpc/connect-web": "^1.3.0",
    "@connectrpc/protoc-gen-connect-es": "^1.3.0",
    "@playwright/test": "^1.41.1",
    "@sveltejs/adapter-auto": "^3.1.1",
    "@sveltejs/kit": "^2.5.0",
    "@testing-library/jest-dom": "^6.4.1",
    "@testing-library/svelte": "^4.1.0",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "flush-promises": "^1.0.2",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.4",
    "prettier-plugin-svelte": "^3.2.2",
    "svelte": "^4.2.9",
    "svelte-check": "^3.6.3",
    "svelte-preprocess": "^5.1.3",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vitest": "^1.2.2"
  }
}
