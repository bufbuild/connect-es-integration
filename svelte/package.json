{
  "name": "buf-svelte",
  "version": "0.0.1",
  "scripts": {
    "start": "npm run buf:generate && vite dev --port 3000",
    "build": "vite build",
    "test": "npm run playwright:install && playwright test",
    "playwright:install": "playwright install",
    "buf:generate": "npx buf generate buf.build/connectrpc/eliza",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --ignore-path ../.prettierignore . && eslint .",
    "format": "prettier --write --ignore-path ../.prettierignore ."
  },
  "type": "module",
  "dependencies": {
    "eslint-plugin-svelte": "^2.32.4"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.25.0",
    "@bufbuild/connect": "^0.12.0",
    "@bufbuild/connect-web": "^0.12.0",
    "@bufbuild/protobuf": "^1.3.0",
    "@bufbuild/protoc-gen-connect-es": "^0.12.0",
    "@bufbuild/protoc-gen-es": "^1.3.0",
    "@playwright/test": "^1.36.2",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.22.4",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^3.0.3",
    "svelte": "^4.1.2",
    "svelte-check": "^3.4.6",
    "svelte-preprocess": "^5.0.4",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.8"
  }
}
