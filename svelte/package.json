{
  "name": "buf-svelte",
  "version": "0.0.1",
  "scripts": {
    "start": "npm run buf:generate && vite dev --port 3000",
    "build": "vite build",
    "test": "npm run playwright:install && playwright test",
    "playwright:install": "playwright install",
    "buf:generate": "npx buf generate buf.build/connectrpc/eliza",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --ignore-path ../.prettierignore . && eslint .",
    "format": "prettier --write --ignore-path ../.prettierignore ."
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.23.1",
    "@bufbuild/connect": "^0.11.0",
    "@bufbuild/connect-web": "^0.11.0",
    "@bufbuild/protobuf": "^1.3.0",
    "@bufbuild/protoc-gen-connect-es": "^0.11.0",
    "@bufbuild/protoc-gen-es": "^1.2.1",
    "@playwright/test": "^1.34.3",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^3.58.0",
    "svelte-check": "^3.4.3",
    "svelte-preprocess": "^5.0.1",
    "tslib": "^2.5.3",
    "typescript": "^5.1.6",
    "vite": "^4.2.3"
  },
  "type": "module"
}
