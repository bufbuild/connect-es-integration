{
  "name": "buf-svelte",
  "version": "0.0.1",
  "scripts": {
    "start": "npm run generate && vite dev",
    "build": "vite build",
    "test": "vitest run",
    "test:e2e": "npm run playwright:install && playwright test",
    "playwright:install": "playwright install",
    "generate": "buf generate buf.build/connectrpc/eliza",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "ci": "npm run generate && npm run check && npm run format && npm run lint && npm run build && npm run test && npm run test:e2e"
  },
  "type": "module",
  "devDependencies": {
    "@bufbuild/buf": "^1.27.2",
    "@bufbuild/protobuf": "^1.4.1",
    "@bufbuild/protoc-gen-es": "^1.4.1",
    "@connectrpc/connect": "^1.1.3",
    "@connectrpc/connect-web": "^1.1.3",
    "@connectrpc/protoc-gen-connect-es": "^1.1.3",
    "@playwright/test": "^1.39.0",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.27.2",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/svelte": "^4.0.5",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.34.0",
    "flush-promises": "^1.0.2",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "svelte": "^4.2.2",
    "svelte-check": "^3.5.2",
    "svelte-preprocess": "^5.0.4",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  }
}
