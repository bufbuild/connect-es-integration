    "@bufbuild/protobuf": "^1.9.0",
    "@bufbuild/protoc-gen-es": "^1.9.0",
    "@connectrpc/connect": "^1.4.0",
    "@connectrpc/connect-web": "^1.4.0",
    "@connectrpc/protoc-gen-connect-es": "^1.4.0",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "esbuild": "^0.20.2",
    "jest": "^29.7.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ts-jest": "^29.1.2",
    "tsc": "^2.0.4",
    "typescript": "^5.4.5"
  }
