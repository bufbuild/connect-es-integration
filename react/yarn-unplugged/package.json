{
  "name": "buf-yarn-unplugged",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "yarn run build && open index.html",
    "build": "esbuild src/index.tsx --bundle --outfile=dist/out.js",
