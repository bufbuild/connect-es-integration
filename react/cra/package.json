{
  "name": "buf-cra",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run buf:generate && react-app-rewired start",
    "build": "BUILD_PATH='./dist' react-app-rewired build",
    "test": "react-app-rewired test --transformIgnorePatterns \"node_modules/(?!connect-web)/\" --watchAll=false",
    "buf:generate": "npx buf generate buf.build/connectrpc/eliza"
  },
  "type": "commonjs",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "(.+)\\.js": "$1"
    }
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.5.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "is-promise": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^3.4.0"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.26.1",
    "@bufbuild/connect": "^0.12.0",
    "@bufbuild/connect-web": "^0.12.0",
    "@bufbuild/protobuf": "^1.3.0",
    "@bufbuild/protoc-gen-connect-es": "^0.12.0",
    "@bufbuild/protoc-gen-es": "^1.3.0",
    "prettier": "^3.0.0",
    "react-app-rewired": "^2.2.1",
    "resolve-typescript-plugin": "^2.0.1",
    "ts-jest": "^29.1.1"
  }
}
