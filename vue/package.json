{
  "name": "buf-vue",
  "version": "0.0.0",
  "scripts": {
    "start": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "test": "vitest run --environment jsdom",
    "buf:generate": "npx buf generate buf.build/bufbuild/eliza",
    "test:e2e": "npm run build && start-server-and-test preview http://127.0.0.1:4173/ 'cypress open --e2e'",
    "test:e2e:ci": "npm run build && start-server-and-test preview http://127.0.0.1:4173/ 'cypress run --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "pinia": "^2.0.27",
    "vue": "^3.2.45",
    "vue-router": "^4.0.16"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.15.0",
    "@bufbuild/connect-web": "^0.8.3",
    "@bufbuild/protobuf": "^1.1.0",
    "@bufbuild/protoc-gen-connect-es": "^0.8.3",
    "@bufbuild/protoc-gen-es": "^1.1.0",
    "@cypress/webpack-preprocessor": "^5.15.2",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^18.11.10",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^12.5.0",
    "eslint": "^8.31.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.8.0",
    "jsdom": "^20.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "resolve-typescript-plugin": "^1.2.0",
    "start-server-and-test": "^1.14.0",
    "ts-loader": "^9.4.2",
    "typescript": "~4.9.5",
    "vite": "^2.9.12",
    "vitest": "^0.25.3",
    "vue-tsc": "^1.0.10"
  }
}
