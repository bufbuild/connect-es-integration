{
  "name": "buf-vue",
  "version": "0.0.0",
  "scripts": {
    "start": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "test": "vitest run --environment jsdom",
    "buf:generate": "npx buf generate buf.build/connectrpc/eliza",
    "test:e2e": "npm run build && start-server-and-test preview http://127.0.0.1:4173/ 'cypress open --e2e'",
    "test:e2e:ci": "npm run build && start-server-and-test preview http://127.0.0.1:4173/ 'cypress run --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "pinia": "^2.1.6",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.26.1",
    "@connectrpc/connect": "^0.13.0",
    "@connectrpc/connect-web": "^0.13.0",
    "@bufbuild/protobuf": "^1.3.0",
    "@connectrpc/protoc-gen-connect-es": "^0.13.0",
    "@bufbuild/protoc-gen-es": "^1.3.0",
    "@cypress/webpack-preprocessor": "^5.17.1",
    "@rushstack/eslint-patch": "^1.3.3",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^20.5.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "cypress": "^12.17.3",
    "eslint": "^8.47.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-vue": "^9.16.1",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.1",
    "resolve-typescript-plugin": "^2.0.1",
    "start-server-and-test": "^2.0.0",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vitest": "^0.34.1",
    "vue-tsc": "^1.8.8"
  }
}
