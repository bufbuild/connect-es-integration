{
  "name": "buf-fastify",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "tsx server.ts",
    "serve": "tsc --noEmit && esbuild webclient.ts --serve=localhost:8080 --servedir=www --outdir=www --bundle --global-name=eliza",
    "client": "tsx client.ts",
    "buf:generate": "npx buf generate buf.build/bufbuild/eliza",
    "build": "tsc --noEmit",
    "test": "NODE_OPTIONS=--experimental-vm-modules ts-node --esm node_modules/.bin/jasmine --config=spec/support/jasmine.json"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.25.0",
    "@bufbuild/connect": "^0.12.0",
    "@bufbuild/connect-fastify": "^0.12.0",
    "@bufbuild/connect-node": "^0.12.0",
    "@bufbuild/connect-web": "^0.12.0",
    "@bufbuild/protobuf": "^1.3.0",
    "@bufbuild/protoc-gen-connect-es": "^0.12.0",
    "@bufbuild/protoc-gen-es": "^1.3.0",
    "@types/jasmine": "^4.3.1",
    "@types/node": "18.11.18",
    "prettier": "^2.8.4",
    "tsx": "^3.12.3",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@fastify/cors": "^8.2.1",
    "fastify": "^4.17.0",
    "jasmine": "^4.6.0",
    "ts-node": "^10.9.1"
  }
}
